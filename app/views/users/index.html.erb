<div class="container">
<div class="row">
 <div class="col-3">
  <%= render 'user', user: @user, book: @book %>
 </div>
 <div class="col-9">
  <h2>Users</h2>
  <table class="table">
   <thead>
    <tr>
     <th>image</th>
     <th>name</th>
    </tr>
   </thead>
   <tbody>
    <% @users.each do |user| %>
    <tr>
     <td><%= image_tag user.get_profile_image(100,100) %></td>
     <td><%= user.name %></td>
    </tr>
   </tbody>
   <% end %>
  </table>
   </tr>
 </div>
</div>
</div>


